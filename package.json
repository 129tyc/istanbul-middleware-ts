{
  "name": "istanbul-middleware-ts",
  "version": "1.0.1",
  "description": "TypeScript version of Istanbul middleware for code coverage reporting",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test-server": "npm run build && node test/server.js",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags",
    "release": "./scripts/release.sh",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "release:beta": "npm version prerelease --preid=beta && npm publish --tag beta",
    "check": "npm run build && npm pack --dry-run",
    "publish:dry": "npm publish --dry-run"
  },
  "keywords": [
    "istanbul",
    "coverage",
    "middleware",
    "typescript",
    "express",
    "testing",
    "code-coverage",
    "nodejs"
  ],
  "author": "Istanbul Middleware TS",
  "repository": {
    "type": "git",
    "url": "https://github.com/129tyc/istanbul-middleware-ts.git"
  },
  "bugs": {
    "url": "https://github.com/129tyc/istanbul-middleware-ts/issues"
  },
  "homepage": "https://github.com/129tyc/istanbul-middleware-ts#readme",
  "license": "BSD-3-Clause",
  "dependencies": {
    "archiver": "^5.3.2",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-reports": "^3.1.6"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.4",
    "@types/express": "^4.17.21",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/node": "^20.10.4",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
